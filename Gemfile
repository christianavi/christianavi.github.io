source "https://rubygems.org"

group :jekyll_plugins do
    gem 'jekyll-feed'
    gem 'jekyll-sitemap'
    gem 'jekyll-paginate'
    gem 'jekyll-seo-tag'
    gem 'jekyll-archives'
    gem 'jekyll-figure'
    gem 'bootstrap', '~> 4.4.1'
    gem 'kramdown'
    gem 'rouge'
end

platforms :mingw, :x64_mingw, :mswin, :jruby do
    gem "tzinfo", "~> 1.2"
    gem "tzinfo-data"
  end
  
  gem "wdm", "~> 0.1.1", :platforms => [:mingw, :x64_mingw, :mswin]
